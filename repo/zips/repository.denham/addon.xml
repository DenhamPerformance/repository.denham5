<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.denham" name="Denham Repository" version="1.0" provider-name="Denham Performance">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
    <extension point="xbmc.addon.repository" name="Example Repository">
            <info compressed="false">http://freedomrodeo.com/_repo/addons.xml</info>
            <checksum>http://freedomrodeo.com/_repo/addons.xml.md5</checksum>
            <datadir zip="true">http://freedomrodeo.com/_repo</datadir>
			<hashes>false</hashes>			
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Denham Repository</summary>
        <description>Denham Builds</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>